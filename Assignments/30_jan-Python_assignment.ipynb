    "ans1:\n",
    "marks=int(input(\"Enter your marks: \"))\n",
    "if marks > 90:\n",
    "    print(\"Grade: A\")\n",
    "elif marks>80 and marks<=90:\n",
    "    print(\"Grade: B\")\n",
    "elif marks>=60 and marks<=80:\n",
    "    print(\"Grade: C\")\n",
    "else:\n",
    "    print(\"Grade: D\")\n",
    "\n",
    "ans2:\n",
    "c = int(input(\"What does your bike cost: \"))\n",
    "def taxCalc(percentage,cost):\n",
    "    tax = (percentage /100)*cost\n",
    "    return tax\n",
    "if c>100000:\n",
    "    tax =taxCalc(15,c)\n",
    "    print(tax)\n",
    "elif c>50000 and c<=100000:\n",
    "    tax = taxCalc(10,c)\n",
    "    print(tax)\n",
    "elif c<=50000:\n",
    "    tax = taxCalc(5,c)\n",
    "    print(tax)\n",
    "\n",
    "ans3:\n",
